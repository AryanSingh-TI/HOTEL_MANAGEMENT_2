<odoo>
    <template id="inherit_tempalte_report" inherit_id="web.external_layout_boxed">
        <xpath expr="//div[contains(@class,'o_boxed_header')]" position="replace">
            <img t-att-src="'https://i.ibb.co/GcnCfHx/Add-a-heading.jpg'" style="margin-top:0%;left:0%;right:0%;width:100%,height:200px;"/>
        </xpath>

        <xpath expr="//div[contains(@t-attf-class,'footer o_boxed_footer')]" position="inside">
            <img t-att-src="'https://img.freepik.com/free-vector/gradient-background-wave-minimalist-style_483537-4961.jpg'"  style="margin-bottom:0%;left:0%;right:0%;width:100%;height:80px;opacity:0.9;z-index:1;"/>
        </xpath>
    </template>
</odoo>